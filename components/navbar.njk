
{% macro navbar(pathlist) %}
    <div class="d-flex navbar-container">
        <div>
            <a href="/">Home</a>
        </div>
        {% for path in pathlist %}
            <div class="path-divider">/</div>
            <div class="path-link">
                <a href="/{{path}}">{{path}}</a>
            </div>
        {% endfor %}
    </div>
{% endmacro %}